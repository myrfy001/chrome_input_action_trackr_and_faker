{
    "manifest_version": 2,

    "name": "InputActionCollector",
    "description": "Collect you mouse and keyboard input for ML training",
    "version": "0.0.1",

    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["page_script.js"]
        }
    ],
    "permissions": [
        "activeTab",
        "https://ajax.googleapis.com/",
        "notifications"   
    ]
}